{
  "name": "sic-web-siar",
  "version": "0.0.0",
  "scripts": {
    "generate-icons": "svg-to-ts",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "build_test": "ng build --configuration=test",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ci:test": "ng test --no-watch --no-progress --code-coverage --browsers=ChromeHeadlessCI",
    "ci:e2e": "ng e2e --protractor-config=e2e/protractor-ci.conf.js",
    "ci:build": "npm audit && npm run build && npm run lint && npm run ci:test && npm run ci:e2e"
  },
  "svg-to-ts": {
    "conversionType": "object",
    "srcFiles": [
      "./src/assets/svg/*.svg"
    ],
    "outputDirectory": "./src/assets/svg",
    "fileName": "svg-icons",
    "svgoConfig": {
      "plugins": [
        {
          "removeDimensions": true,
          "cleanupAttrs": true
        }
      ]
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.2.3",
    "@angular/cdk": "10.1.3",
    "@angular/common": "10.2.3",
    "@angular/compiler": "10.2.3",
    "@angular/core": "10.2.3",
    "@angular/flex-layout": "10.0.0-beta.32",
    "@angular/forms": "10.2.3",
    "@angular/material": "10.1.3",
    "@angular/platform-browser": "10.2.3",
    "@angular/platform-browser-dynamic": "10.2.3",
    "@angular/router": "10.2.3",
    "@fullcalendar/angular": "5.3.1",
    "@fullcalendar/daygrid": "5.3.2",
    "@fullcalendar/interaction": "5.3.1",
    "@ngneat/svg-icon": "1.0.0",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "crypto-js": "4.0.0",
    "d3": "6.2.0",
    "datebook": "5.0.2",
    "file-saver": "2.0.5",
    "hammerjs": "2.0.8",
    "moment": "2.27.0",
    "ng-inline-svg": "10.1.0",
    "ng-recaptcha": "6.0.2",
    "ngx-countdown": "10.0.1",
    "ngx-extended-pdf-viewer": "9.0.2",
    "ngx-pagination": "5.0.0",
    "ngx-spinner": "10.0.1",
    "ngx-toastr": "13.0.1",
    "ngx-translate-multi-http-loader": "3.0.0",
    "ngx-ui-loader": "10.0.0",
    "plotly.js": "1.47.4",
    "rxjs": "6.6.2",
    "tslib": "2.0.0",
    "xlsx": "0.16.9",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1000.8",
    "@angular/cli": "10.0.8",
    "@angular/compiler-cli": "10.2.3",
    "@types/jasmine": "3.5.0",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "14.6.1",
    "codelyzer": "6.0.0",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "5.0.0",
    "karma": "5.1.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.2",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.0",
    "protractor": "7.0.0",
    "svg-to-ts": "5.0.0",
    "ts-node": "9.0.0",
    "tslint": "6.1.0",
    "typescript": "3.9.7"
  }
}
